{
  "version": "1",
  "auth": {
    "user_pool_id": "REPLACE_WITH_YOUR_USER_POOL_ID",
    "aws_region": "us-east-1",
    "user_pool_client_id": "REPLACE_WITH_YOUR_CLIENT_ID",
    "identity_pool_id": "REPLACE_WITH_YOUR_IDENTITY_POOL_ID",
    "mfa_methods": [],
    "standard_required_attributes": ["email"],
    "username_attributes": ["email"],
    "user_verification_types": ["email"],
    "mfa_configuration": "OPTIONAL",
    "password_policy": {
      "min_length": 8,
      "require_lowercase": true,
      "require_uppercase": true,
      "require_numbers": true,
      "require_symbols": true
    }
  },
  "data": {
    "url": "REPLACE_WITH_YOUR_APPSYNC_ENDPOINT",
    "aws_region": "us-east-1",
    "default_authorization_type": "AMAZON_COGNITO_USER_POOLS",
    "authorization_types": ["AWS_IAM"]
  },
  "storage": {
    "aws_region": "us-east-1",
    "bucket_name": "REPLACE_WITH_YOUR_BUCKET_NAME"
  }
}
