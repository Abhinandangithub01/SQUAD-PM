# Production environment variables for Vercel deployment
REACT_APP_API_URL=https://your-api-domain.vercel.app/api
REACT_APP_SOCKET_URL=wss://your-api-domain.vercel.app
REACT_APP_ENVIRONMENT=production

# Build optimizations
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Cache busting
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_TIMESTAMP=$VERCEL_GIT_COMMIT_SHA

# Performance optimizations
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
